<UserControl x:Class="DDECAD.MZ.Classes.GUI.Windows.MzAddStick"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DDECAD.MZ.Classes.GUI.Windows"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="300"
                        ></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        
        <TextBlock Grid.Row="0" Text="New Stick" FontSize="23" Foreground="Bisque"
                   VerticalAlignment="Center" HorizontalAlignment="Center"
                   >
            
        </TextBlock>

        <Button Name="ButtonAddStick" Grid.Column="0"  Grid.Row="1"  Click="ButtonAddStick_Click">            
        </Button>
            
    </Grid>
</UserControl>
